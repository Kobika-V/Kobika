def triangle_type(side1, side2, side3):
    if side1 == side2 == side3:
        return "That's a equilateral triangle"
    elif side1 == side2 or side1 == side3 or side2 == side3:
        return "That's a isosceles triangle"
    else:
        return "That's a scalene triangle"
side1 = int(input())
side2 = int(input())
side3 = int(input())

triangle = triangle_type(side1, side2, side3)
print(triangle)
