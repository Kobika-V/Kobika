def check_vowel_consonant(letter):
    if letter.lower() in ['a', 'e', 'i', 'o', 'u']:
        return "It's a vowel."
    elif letter.lower() == 'y':
        return "Sometimes it's a vowel... Sometimes it's a consonant."
    else:
        return "It's a consonant."
letter = input()
result = check_vowel_consonant(letter)
print(result)
