def find_pair_with_difference(arr, k):
    i, j = 0, 1
    
    while j < len(arr):
        diff = arr[j] - arr[i]
        if diff == k:
            return 1
        elif diff < k:
            j += 1
        else:
            i += 1
            if i == j:
                j += 1
                
    return 0

# Input
T = int(input())
for _ in range(T):
    
    N = int(input())
    array = [int(input()) for i in range(N)]
    
    k = int(input())
    
    
    # Output
    print(find_pair_with_difference(array, k))
